{
  "name": "numlab",
  "description": "A comprehensive and versatile library for all your mathematical operations and computations.",
  "version": "0.1.7",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./arithmetic": {
      "import": "./dist/arithmetic.esm.js",
      "require": "./dist/arithmetic.cjs.js",
      "types": "./dist/arithmetic.d.ts"
    },
    "./advancedMath": {
      "import": "./dist/advancedMath.esm.js",
      "require": "./dist/advancedMath.cjs.js",
      "types": "./dist/advancedMath.d.ts"
    },
    "./linearAlgebra": {
      "import": "./dist/linearAlgebra.esm.js",
      "require": "./dist/linearAlgebra.cjs.js",
      "types": "./dist/linearAlgebra.d.ts"
    },
    "./logarithms": {
      "import": "./dist/logarithms.esm.js",
      "require": "./dist/logarithms.cjs.js",
      "types": "./dist/logarithms.d.ts"
    },
    "./random": {
      "import": "./dist/random.esm.js",
      "require": "./dist/random.cjs.js",
      "types": "./dist/random.d.ts"
    },
    "./statistics": {
      "import": "./dist/statistics.esm.js",
      "require": "./dist/statistics.cjs.js",
      "types": "./dist/statistics.d.ts"
    },
    "./trigonometry": {
      "import": "./dist/trigonometry.esm.js",
      "require": "./dist/trigonometry.cjs.js",
      "types": "./dist/trigonometry.d.ts"
    }
  },
  "homepage": "https://github.com/aarabii/numlab#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/aarabii/numlab.git"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "keywords": [
    "math",
    "npm",
    "calculation",
    "number",
    "mathematics",
    "maths",
    "mathematical",
    "operations",
    "computation",
    "arithmetic",
    "functions",
    "library",
    "scientific",
    "calculator",
    "algorithm",
    "numeric",
    "formula",
    "mathematician",
    "calculator",
    "mathematic",
    "math operator"
  ],
  "author": "caya",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  }
}
